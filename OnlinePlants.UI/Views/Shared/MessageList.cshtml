



<div id="titlebar" class="single">
    <div class="container">
        <div class="sixteen columns">
            <h2>Inbox</h2>
        </div>
    </div>
</div>

<div class="container" ng-app="Messages" ng-controller="MessagesController" ng-init="fn_GetMessageList()">
    <div ng-repeat="msg in MessageList">
        <div class="application" style="margin:0; margin-top:0.5em; ">
            <div class="app-content" style="padding:0px 0px 10px 15px !important; ">
                <div class="info">
                    <span style="font-size: 14px; width:30%;">{{msg.Name}}</span>
                    <span style="font-size: 14px; cursor:pointer;" ng-click="fn_OpenThread(msg.ReceiverId, msg.JobId)"> Subject : Response against {{msg.Subject}}  </span>
                </div>
            </div>
        </div>
    </div>
</div>
